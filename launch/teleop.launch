<launch>
	<node pkg="key_teleop" type="key_teleop.py" name="eod_teleop" output="screen">
		
		<param name="hz" value="10" />
		<param name="~turbo/steps" value="20" />
		
		<param name="~turbo/linear_forward_min" value="0.4" />
		<param name="~turbo/linear_forward_max" value="1.0" />
		
		<param name="~turbo/linear_backward_min" value="0.4" />
		<param name="~turbo/linear_backward_max" value="0.7" />

		<param name="~turbo/angular_min" value="0.4" />
		<param name="~turbo/angular_max" value="0.8" />

		<remap from="key_vel" to="cmd_vel" />
	</node>
</launch>